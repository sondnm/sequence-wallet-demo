{
  "name": "smart-wallet-demo",
  "version": "1.0.0",
  "description": "Demo of the sequence smart wallet on local",
  "scripts": {
    "build": "rm -rf build && tsc -p .",
    "deploy": "yarn build && node build/index.js deploy",
    "balance": "yarn build && node build/index.js balance",
