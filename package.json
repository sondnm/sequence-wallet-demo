  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "0xsequence": "^0.40.4",
    "@0xsequence/deployer": "^0.40.4",
    "@0xsequence/wallet-contracts": "^1.9.6",
    "commander": "^9.4.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9"
  },
  "main": "./build/index.js",
  "bin": {
    "smart-wallet": "build/index.js"
  },
  "author": "sondnm",
